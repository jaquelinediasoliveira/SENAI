package visao;

public class Viagem {
	//Atributos
	String origem;
	String destino;
	int distancia;
	float tempo;
	//Ações
	public float getVelocidade() { //Método
		return distancia/tempo;
	}
	//Construtor
	public Viagem(String origem, String destino, int distancia, float tempo) {
		this.origem = origem;
		this.destino = destino;
		this.distancia = distancia;
		this.tempo = tempo;
	}
}
